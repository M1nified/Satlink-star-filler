{
  "author": "Michał Góra - ProWebJect.com",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "all_frames": false,
      "js": [
        "jquery.js",
        "run_in_browser.js"
      ],
      "matches": [
        "http://statlink.pl/*"
      ],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2,
  "name": "Satlink star filler",
  "page_action": {
    "default_title": "Try the stars!"
  },
  "permissions": [
    "http://statlink.pl/*"
  ],
  "version": "0.3"
}
